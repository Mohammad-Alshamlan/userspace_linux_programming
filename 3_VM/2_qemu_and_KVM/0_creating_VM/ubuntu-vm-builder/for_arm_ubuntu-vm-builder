	$ sudo vmbuilder kvm ubuntu --suite trusty  --addpkg=linux-image-generic --addpkg=bash-completion --addpkg=openssh-server
	
[2] setup "alshamlan" username and password when building with "vmbuilder"
	
	$ sudo vmbuilder qemu ubuntu --arch arm --suite trusty  --addpkg=linux-image-generic --addpkg=bash-completion --addpkg=openssh-server --user alshamlan --pass 288071100637

[3]

	vmbuilder kvm ubuntu \
	--mem=1024 \
	--cpus=1 \
	--hostname=vm01 \
	--dest=vm01 \
	--mirror=http://192.168.122.1:3142/ubuntu \
	--user=wikiadmin \
	--name=administrator \
	--pass=secret \
	--rootpass=verysecret \
	--libvirt=qemu:///system \
	--rootsize=2048 \
	--swapsize=512 \
	--overwrite \
	--tmpfs -

	
